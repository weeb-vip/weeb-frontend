{
  "name": "transplex-frontend",
  "private": true,
  "version": "1.5.3",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "start": "node ./dist/server/entry.mjs",
    "test": "jest",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:mobile": "playwright test tests/e2e/mobile.spec.ts",
    "test:e2e:registration": "playwright test tests/e2e/registration.spec.ts",
    "test:e2e:registration:headed": "playwright test tests/e2e/registration.spec.ts --headed",
    "test:e2e:registration:direct": "playwright test tests/e2e/registration-direct.spec.ts",
    "test:e2e:ios": "IOS_SIMULATOR=true playwright test --project=ios-simulator",
    "test:e2e:android": "ANDROID_EMULATOR=true playwright test --project=android-emulator",
    "test:e2e:report": "playwright show-report",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "update-deployment-tags": "node scripts/update-deployment-tags.js",
    "deploy:staging": "node scripts/deploy-cloudflare.js staging",
    "deploy:production": "node scripts/deploy-cloudflare.js production",
    "deploy:cf": "node scripts/deploy-cloudflare.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weeb-vip/weeb-frontend.git"
  },
  "dependencies": {
    "@algolia/autocomplete-core": "^1.19.4",
    "@algolia/autocomplete-js": "^1.9.2",
    "@algolia/autocomplete-preset-algolia": "^1.19.4",
    "@apollo/client": "^3.7.14",
    "@astrojs/cloudflare": "^12.6.8",
    "@astrojs/node": "^9.4.3",
    "@astrojs/svelte": "^7.1.1",
    "@astrojs/tailwind": "^6.0.2",
    "@date-fns/tz": "^1.4.1",
    "@date-fns/utc": "^2.1.0",
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@popperjs/core": "^2.11.7",
    "@rgossiaux/svelte-headlessui": "^2.0.0",
    "@tanstack/svelte-query": "^5.89.0",
    "@types/graphql": "^14.5.0",
    "algoliasearch": "^5.37.0",
    "astro": "^5.13.7",
    "axios": "^1.3.4",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.0.3",
    "flagsmith": "^3.18.3",
    "graphql": "^16.6.0",
    "graphql-request": "^6.0.0",
    "he": "^1.2.0",
    "motion": "^12.10.5",
    "qa": "^0.0.1",
    "query-string": "^8.1.0",
    "sass": "^1.62.1",
    "socket.io-client": "^4.6.1",
    "svelte": "^5.38.10",
    "svelte-fa": "^4.0.4",
    "svelte-sonner": "^1.0.5",
    "swr": "^2.1.2",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.3",
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/client-preset": "^3.0.1",
    "@jest/globals": "^30.1.1",
    "@playwright/test": "^1.55.0",
    "@redux-devtools/extension": "^3.3.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@types/he": "^1",
    "@types/jest": "^30.0.0",
    "@types/node": "^18.15.11",
    "@types/qs": "^6.9.7",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.14",
    "jest": "^30.1.1",
    "postcss": "^8.4.21",
    "semantic-release": "^17.4.7",
    "storybook": "^8.4.7",
    "tailwindcss": "^3.3.1",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.4",
    "uuid": "^12.0.0",
    "vite": "^6.3.5",
    "vite-plugin-pwa": "^1.0.3",
    "vite-plugin-static-copy": "^0.13.1",
    "wrangler": "^4.38.0"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "packageManager": "yarn@4.10.2"
}
